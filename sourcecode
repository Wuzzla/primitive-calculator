public class Taschenrechner 
{
    double zahl1;
    double zahl2;
    
    public double addieren() {
        return zahl1 + zahl2;
    }
    
    public double subtrahieren() {
        return  zahl1 - zahl2;
    }
    
    public double multiplizieren() {
        return  zahl1 * zahl2;
    }
    
    public double dividieren() {
        return  zahl1 / zahl2;
    }
    
    public void setZahl1(double zahl1) {
        this.zahl1 = zahl1;
    }
    
    public void setZahl2(double zahl2) {
        this.zahl2 = zahl2;
    }
    
}
